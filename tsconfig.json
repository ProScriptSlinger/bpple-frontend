{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "lib/utils.js", "lib/getVersionedTransaction.js", "lib/getProvider.js", "lib/formatAddress.js", "lib/signAndConfirmTransaction.js", "app/create-nft/page.jsx", "components/Modal/NftUnListModal.jsx", "components/Modal/NftListModal.jsx", "app/marketplace/[id]/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
